{
  "name": "starter-webapp",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "build": "vite build",
    "build:ssg": "vite-ssg build",
    "dev": "vite --port 3333",
    "preview": "vite preview",
    "lint": "oxlint && eslint",
    "lint:fix": "oxlint --fix && eslint --fix",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^8.12.0",
    "@unhead/vue": "^1.9.14",
    "axios": "^1.7.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.30",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@quasar/extras": "^1.16.11",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.8",
    "@unhead/addons": "^1.9.14",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vueuse/core": "^10.11.0",
    "critters": "^0.0.24",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-oxlint": "^0.4.0",
    "lint-staged": "^15.2.7",
    "oxlint": "^0.4.4",
    "prettier": "^3.3.2",
    "quasar": "^2.16.4",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "^0.19.0",
    "unplugin-imagemin": "^0.5.18",
    "unplugin-vue-components": "^0.27.1",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-webfont-dl": "^3.9.4",
    "vite-ssg": "^0.23.7",
    "vite-ssg-sitemap": "^0.7.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint:fix"
  }
}
